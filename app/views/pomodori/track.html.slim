section.pomodoro
  header
    h1 Pomodoro Tracking
    .countdown

- content_for :inline
  coffee:
    $ -> 
      $('.countdown').jCountdown 
        timeText:"2020/01/01 00:00:00"
        timeZone:8
        style:"flip"
        color:"black"
        width:0
        textGroupSpace:15
        textSpace:0
        reflection:true
        reflectionOpacity:10
        reflectionBlur:0
        dayTextNumber:4
        displayDay:true
        displayHour:true
        displayMinute:true
        displaySecond:true
        displayLabel:true
        onFinish: ->
          alert("finish")
